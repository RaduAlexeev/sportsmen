{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off",
        "indent": [
            "off",
            4
        ],
        "semi": 2,
        "quotes": 2,
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-mixed-spaces-and-tabs": "error",
        "react/react-in-jsx-scope": 0,
        "react/jsx-uses-react": 0
    }
}